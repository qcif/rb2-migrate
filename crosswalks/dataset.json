{
  "idfield": "oid",
  "source_type": "dataset",
  "dest_type": "dataset",
  "publish": true,
  "permissions": {
    "user_id": "email",
    "permissions": {
      "_owner_": [
        "view",
        "edit"
      ],
      "contributor_ci": [
        "view",
        "edit"
      ],
      "contributor_data_manager": [
        "view",
        "edit"
      ],
      "contributors": [
        "view"
      ],
      "contributor_supervisor": [
        "view"
      ]
    }
  },
  "required": [
    "title",
    "description"
  ],
  "ignore": [
  ],
  "fields": {
    "dc:title": "title",
    "dc_description": "description",
    "foaf:fundedBy.foaf:Agent": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "FundingBody",
      "fields": {
        "dc:identifier": "dc_identifier",
        "skos:prefLabel": "dc_title"
      }
    },
    "foaf:fundedBy.vivo:Grant": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "GrantBody",
      "fields": {
        "dc:identifier": "dc_identifier",
        "skos:prefLabel": "dc_title",
        "redbox:grantNumber": "grant_number"
      }
    },
    "locrel:prc.foaf:Person": {
      "name": "contributor_data_manager",
      "type": "record",
      "handler": "Person",
      "role": "Data Manager",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },
    "dc:contributor.locrel:clb": {
      "name": "contributors",
      "type": "record",
      "handler": "SingleTextFieldPerson",
      "repeatable": 1,
      "additive": 1,
      "role": "Collaborator",
      "fields": {
        "foaf:Agent": "fullname"
      }
    },
    "swrc:supervisor.foaf:Person": {
      "name": "contributor_supervisor",
          "role": "Advisor",
      "type": "record",
      "handler": "Person",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },
    "dc:creator.foaf:Person": {
      "destinations": [
        {
          "to": "contributor_ci",
          "singleUse": true
        },
        {
          "repeatable": true
        }
      ],
      "type": "record",
      "defaultRole": "Chief Investigator",
      "defaultField": "contributor_oi",
      "repeatable": 1,
      "handler": "AffiliatedPerson",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },
    "bibo:Website": {
      "name": "dataLocations",
      "type": "record",
      "additive": 1,
      "repeatable": 1,
      "handler": "URLDataLocations",
      "fields": {
        "dc:identifier": "location",
        "type": "type",
        "skos:note": "notes"
      }
    },
    "vivo:Location.vivo:GeographicLocation": {
      "name": "dataLocations",
      "additive": 1,
      "type": "record",
      "repeatable": 1,
      "handler": "PhysicalDataLocations",
      "fields": {
        "gn:name": "location",
        "type": "type",
        "skos:note": "notes"
      }
    },
    "dc:relation.swrc:Publication": {
      "name": "related_publications",
      "type": "record",
      "repeatable": 1,
      "fields": {
        "dc:identifier": "related_url",
        "dc:title": "related_title",
        "skos:note": "related_notes"
      }
    },
    "dc:type.rdf:PlainLiteral": {
      "name": "datatype",
      "type": "valuemap",
      "map": {
        "catalogueOrIndex": "catalogueOrIndex",
        "collection": "collection",
        "dataset": "dataset",
        "registry": "registry",
        "repository": "repository"
      }
    },
    "redbox:retentionPeriod": {
      "name": "redbox:retentionPeriod_dc:date",
      "type": "valuemap",
      "map": {
        "1year": "1year",
        "5years": "5years",
        "7years": "7years",
        "10years": "10years",
        "15years": "15years",
        "20years": "20years",
        "permanent": "permanent",
        "12 months (minor project)": "1year",
        "5 years (minimum for projects with publications)": "5years",
        "5 years after publication (general research)": "5years",
        "5 Years": "5years",
        "7 years": "7years",
        "10 years": "10years",
        "15 years (all clinical trials)": "15years",
        "15 years": "15years",
        "20 years": "20years",
        "Indefinitely": "permanent",
        "indefinitely": "permanent",
        "indefinite": "permanent",
        "Retain indefinitely": "permanent",
        "Retained indefinitely": "permanent",
        "Perpetuity": "permanent",
        "Indefinite": "permanent",
        "Seven years (beginning December 2012)": "7years",
        "1": "1year",
        "5": "5years",
        "7": "7years",
        "15": "15years",
        "20": "20years"
      }
    },
    "redbox:retentionPeriod.dc:date.skos:note": {
      "name": "_",
      "type": "valuemap",
      "map": {
        "heritage": "heritage",
        "controversial": "controversial",
        "ofinterest": "ofinterest",
        "costly_impossible": "costly_impossible",
        "It would be costly or impossible to reproduce": "costly_impossible",
        "commercial": "commercial"
      }
    },
    "redbox:disposalDate": "disposalDate",
    "locrel:own.foaf:Agent": {
      "name": "data_owners",
      "type": "record",
      "handler": "FlatKeywords",
      "repeatable": 1,
      "fields": {
        "foaf:name": ""
      }
    }
  }
}

