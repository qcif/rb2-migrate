{
  "idfield": "oid",
  "source_type": "dataset",
  "workflow_step": "live",
  "dest_type": "dataPublication",
  "workflow": {
    "stageold1": "stagenew1",
    "stageold2": "stagenew2"
  },
  "permissions": {
    "user_id": "email",
    "permissions": {
      "_owner_": [
        "view",
        "edit"
      ],
      "contributor_ci": [
        "view",
        "edit"
      ],
      "contributor_data_manager": [
        "view",
        "edit"
      ],
      "contributors": [
        "view"
      ],
      "contributor_supervisor": [
        "view"
      ]
    }
  },
  "required": [
    "title",
    "description"
  ],
  "ignore": [
    "action",
    "oid",
    "objMeta"
  ],
  "postTasks": [
    {
      "name": "complement",
      "fields": [
        {
          "name": "contributor_data_manager",
          "updateTo": "contributor_ci"
        }
      ]
    },
    {
      "name": "removeIfRepeated",
      "fields": [
        {
          "name": "contributor_ci",
          "removeFrom": "contributors",
          "compare": "dc:identifier"
        },
        {
          "name": "contributor_data_manager",
          "removeFrom": "contributors",
          "compare": "dc:identifier"
        }
      ]
    }
  ],
  "fields": {
    "dc:creator.foaf:Person": {
      "name": "creators",
      "type": "record",
      "handler": "Person",
      "role": "",
      "repeatable": 1,
      "destIsArray": true,
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "given_name",
        "foaf:familyName": "family_name"
      }
    },
    "dc_title": "title",
    "dc:title": "title",
    "description": "description",
    "dc:description": "description",
    "dc:subject.vivo:keyword": {
      "name": "finalKeywords",
      "type": "record",
      "repeatable": 1,
      "handler": "FlatKeywords",
      "fields": {
        "rdf:PlainLiteral": ""
      }
    },
    "bibo:DataLocation": {
      "name": "dataLocations",
      "type": "record",
      "handler": "DataLocations",
      "repeatable": 1,
      "fields": {
        "dc:location": "location",
        "dc:type": "type",
        "dc:privacy": "privacy",
        "dc:notes": "notes"
      }
    },
    "redbox:retentionPeriod": {
      "name": "redbox:retentionPeriod_dc:date",
      "type": "valuemap",
      "map": {
        "1year": "1year",
        "5years": "5years",
        "7years": "7years",
        "25years": "25years",
        "permanent": "permanent"
      }
    },
    "foaf:fundedBy.foaf:Agent": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "FundingBody",
      "fields": {
        "dc:identifier": "dc_identifier",
        "skos:prefLabel": "dc_title"
      }
    },
    "foaf:fundedBy.vivo:Grant": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "GrantBody",
      "fields": {
        "warning": "mint_lookup",
        "dc:identifier": "dc:identifier",
        "skos.prefLabel": "dc:title",
        "redbox:grantNumber": "grant_number"
      }
    },
    "dc:subject.anzsrc:for": {
      "name": "dc:subject_anzsrc:for",
      "type": "record",
      "handler": "ForSeo",
      "repeatable": 1,
      "fields": {
        "skos:prefLabel": "skos:prefLabel",
        "rdf:resource": "rdf:resource"
      }
    },
    "dc:accessRights": {
      "name": "_",
      "type": "valuemap",
      "map": {
        "permission from the data manager": "manager",
        "open, free access under license": "open",
        "no access due to sensitivities": "none"
      }
    },
    "dc:type.rdf:PlainLiteral": {
      "name": "dc:subject_anzsrc:toa_rdf:resource",
      "type": "valuemap",
      "map": {
        "catalogueOrIndex": "catalogueOrIndex",
        "collection": "collection",
        "dataset": "dataset",
        "registry": "registry",
        "repository": "repository",
        "software": "software"
      }
    },
    "dc:coverage.vivo:DateTimeInterval.vivo:start": {
      "name": "startDate",
      "type": "record",
      "handler": "DateTime",
      "fromFormat": "YYYY-MM-DD",
      "toFormat":"YYYY/MM/DD"
    },
    "dc:coverage.vivo:DateTimeInterval.vivo:end": {
      "name": "endDate",
      "type": "record",
      "handler": "DateTime",
      "fromFormat": "YYYY-MM-DD",
      "toFormat":"YYYY/MM/DD"
    },
    "dc:coverage.vivo:GeographicLocation": {
      "name": "geolocations",
      "type": "record",
      "repeatable": 1,
      "fields": {
        "dc:identifier": "identifier",
        "rdf:PlainLiteral": "basic_name",
        "geo:lat": "lat",
        "geo:long": "long"
      }
    },
    "redbox:embargo.redbox:isEmbargoed": "embargoByDate",
    "redbox:embargo.dc:date": "embargoUntil"
  }
}
