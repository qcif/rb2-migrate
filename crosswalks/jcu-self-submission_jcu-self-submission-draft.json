        "warning": "mint_lookup",
        "dc:identifier": "dc_identifier",
        "skos.prefLabel": "dc_title",
        "redbox:grantNumber": "grant_number"
      }
    },
    "locrel:prc.foaf:Person": {
      "destinations": [
        {
          "to": "contributor_data_manager",
          "role": "Data Manager"
        },
        {
          "repeatable": true,
          "role": "Data Manager"
        }
      ],
      "type": "record",
      "defaultRole": "Contributors",
      "defaultField": "contributors",
      "repeatable": 1,
      "handler": "Person",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },
    "dc:contributor.locrel:clb": {
      "name": "contributors",
      "type": "record",
      "handler": "SingleTextFieldPerson",
      "repeatable": 1,
      "additive": 1,
      "role": "Collaborators",
      "fields": {
        "foaf:Agent": "fullname"
      }
    },
    "swrc:supervisor.foaf:Person": {
      "destinations": [
        {
          "to": "contributor_supervisor",
          "role": "Adviser"
        },
        {
          "repeatable": true,
          "role": "Adviser"
        }
      ],
      "type": "record",
      "defaultRole": "Contributors",
      "defaultField": "contributors",
      "repeatable": 1,
      "handler": "Person",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },
    "dc:creator.foaf:Person": {
      "destinations": [
        {
          "to": "contributor_ci",
          "role": "Chief Investigator"
        },
        {
          "repeatable": true,
          "role": "Chief Investigator"
        }
      ],
      "type": "record",
      "defaultRole": "Contributors",
      "defaultField": "contributors",
      "repeatable": 1,
      "handler": "AffiliatedPerson",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname",
        "foaf:Organization.dc:identifier": "affiliationA",
        "foaf:Organization.B.dc:identifier": "affiliationB",
        "foaf:Organization.C.dc:identifier": "affiliationC",
        "jcu:relationshipType": "relationshipType"
      }
    },
    "bibo:Website": {
      "name": "dataLocations",
      "type": "record",
      "additive": 1,
      "repeatable": 1,
      "handler": "URLDataLocations",
      "fields": {
        "dc:identifier": "location",
        "type": "type"
      }
    },
    "vivo:Location.vivo:GeographicLocation": {
      "name": "dataLocations",
      "additive": 1,
      "type": "record",
      "repeatable": 1,
      "handler": "PhysicalDataLocations",
      "fields": {
        "gn:name": "location",
        "type": "type"
      }
    },
    "dc:relation.swrc:Publication": {
      "name": "related_publications",
      "type": "record",
      "repeatable": 1,
      "fields": {
        "dc:identifier": "related_url",
        "dc:title": "related_title",
        "skos:note": "related_notes"
      }
    },
    "dc:type.rdf:PlainLiteral": {
      "name": "datatype",
      "type": "valuemap",
      "map": {
        "catalogueOrIndex": "catalogueOrIndex",
        "collection": "collection",
        "dataset": "dataset",
        "registry": "registry",
        "repository": "repository"
      }
    },
    "dc:subject.anzsrc:for": {
      "name": "dc:subject_anzsrc:for",
      "type": "record",
      "handler": "ForSeo",
      "repeatable": 1,
      "fields": {
        "skos:prefLabel": "skos:prefLabel",
        "rdf:resource": "rdf:resource"
      }
    },
    "dc:subject.anzsrc:seo": {
      "name": "dc:subject_anzsrc:seo",
      "type": "record",
      "handler": "ForSeo",
      "repeatable": 1,
      "fields": {
        "skos:prefLabel": "skos:prefLabel",
        "rdf:resource": "rdf:resource"
      }
    },
    "redbox:retentionPeriod": {
      "name": "redbox:retentionPeriod_dc:date",
      "type": "valuemap",
      "map": {
        "1year": "1year",
        "5years": "5years",
        "7years": "7years",
        "15years": "15years",
        "20years": "20years",
        "permanent": "permanent",
        "12 months (minor project)": "1year",
        "5 years (minimum for projects with publications)": "5years",
        "7 years": "7years",
        "15 years (all clinical trials)": "15years",
        "Indefinitely": "permanent",
        "indefinitely": "permanent"
      }
    },
    "redbox:retentionPeriod.dc:date.skos:note": {
      "name": "_",
      "type": "valuemap",
      "map": {
        "heritage": "heritage",
        "controversial": "controversial",
        "ofinterest": "ofinterest",
        "costly_impossible": "costly_impossible",
        "commercial": "commercial"
      }
    },
    "redbox:disposalDate": "disposalDate"
  }
}

