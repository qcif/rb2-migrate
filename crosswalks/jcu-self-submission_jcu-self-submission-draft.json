      "name": "finalKeywords",
      "type": "record",
      "handler": "FlatKeywords",
      "repeatable": 1,
      "fields": {
        "rdf:PlainLiteral": ""
      }
    },
    "dc:description": {
      "type": "record",
      "name": "description",
      "handler": "HTMLLessFirstFullDescription",
      "fields": {
        "text": "text",
        "type": "type"
      }
    },
    "foaf:fundedBy.vivo:Grant": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "GrantBody",
