{
  "idfield": "oid",
  "source_type": "jcu-self-submission",
  "workflow_step": "jcu-self-submission-draft",
  "dest_type": "dataRecord",
  "permissions": {
    "user_id": "email",
    "permissions": {
      "_owner_": [
        "view",
        "edit"
      ],
      "contributor_ci": [
        "view",
        "edit"
      ],
      "contributor_data_manager": [
        "view",
        "edit"
      ],
      "contributors": [
        "view"
      ],
      "contributor_supervisor": [
        "view"
      ]
    }
  },
  "required": [
    "title",
    "description",
    "finalKeywords",
    "datatype",
    "contributor_ci",
    "contributor_data_manager"
  ],
  "ignore": [
    "action",
    "oid",
    "objMeta"
  ],
  "fields": {
    "dc:title": "title",
