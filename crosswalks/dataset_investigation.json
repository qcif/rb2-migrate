{
  "idfield": "oid",
  "source_type": "dataset",
  "workflow_step": "investigation",
  "dest_type": "dataRecord",
  "workflow": {
    "stageold1": "stagenew1",
    "stageold2": "stagenew2"
  },
  "permissions": {
    "user_id": "email",
    "permissions": {
      "_owner_": [
        "view",
        "edit"
      ],
      "contributor_ci": [
        "view",
        "edit"
      ],
      "contributor_data_manager": [
        "view",
        "edit"
      ],
      "contributors": [
        "view"
      ],
      "contributor_supervisor": [
        "view"
      ]
    }
  },
  "required": [
    "title",
    "description",
    "finalKeywords"
  ],
  "ignore": [
    "action",
    "oid",
    "objMeta"
